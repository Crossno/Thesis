	keywords = {Condensed Matter - Mesoscale and Nanoscale Physics, Condensed Matter - Statistical Mechanics, Condensed Matter - Strongly Correlated Electrons, Condensed Matter - Superconductivity, noise, shot noise, thesis},
	pages = {1--166},
	file = {arXiv\:cond-mat/9910158 PDF:/home/crossno/.zotero/zotero/ioefnje5.default/zotero/storage/TMQ26GBN/Blanter and Buttiker - 2000 - Shot Noise in Mesoscopic Conductors.pdf:application/pdf;arXiv.org Snapshot:/home/crossno/.zotero/zotero/ioefnje5.default/zotero/storage/AAW3426B/9910158.html:text/html}
