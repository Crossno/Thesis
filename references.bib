	month = feb,
	year = {2013},
	keywords = {Condensed-matter physics, graphene, Graphene, supercollision, thermal, thesis},
	pages = {109--112},
	file = {Full Text PDF:/home/crossno/.zotero/zotero/ioefnje5.default/zotero/storage/9C3RW6FI/Betz et al. - 2013 - Supercollision cooling in undoped graphene.pdf:application/pdf;Snapshot:/home/crossno/.zotero/zotero/ioefnje5.default/zotero/storage/9TKZ2XZ9/nphys2494.html:text/html}
