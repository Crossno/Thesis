
@article{yoshino_significant_2015,
	title = {Significant enhancement of electronic thermal conductivity of two-dimensional zero-gap systems by bipolar-diffusion effect},
	volume = {84},
	issn = {0031-9015},
	url = {http://journals.jps.jp/doi/abs/10.7566/JPSJ.84.024601},
	doi = {10.7566/JPSJ.84.024601},
	abstract = {Electrical conductivity (σ), thermopower (S), electronic thermal conductivity (κe), and Lorentz ratio (L = κe/σT) are examined for two-dimensional (2D) zero-gap systems. The bipolar-diffusion (BD) effect in 2D Dirac cones is discussed for the first time. Semiclassical calculations show that L is enhanced by a factor of 200–400\% as compared with the conventional Sommerfeld value L0 = 2.45 × 10−8 W·Ω·K−2 due to the BD effect. It is, however, shown that this enhancement is not due to the linear dispersion of the Dirac cones but due to the zero band gap and is widely expected to occur in all zero-gap systems under its charge neutrality condition. Calculations taking into account electron–hole puddles revealed that the BD effect is suppressed in pristine graphene devices affected by charged-impurity potentials.},
	number = {2},
	journal = {Journal of the Physical Society of Japan},
	author = {Yoshino, H. and Murata, K.},
	month = jan,
	year = {2015},
	keywords = {thesis},
	pages = {024601},
	file = {Snapshot:/home/crossno/.zotero/zotero/ioefnje5.default/zotero/storage/J249ZXUE/JPSJ.84.html:text/html}
}

